  FormBox[
   RowBox[{
    SuperscriptBox["\[ExponentialE]", "x"], "-", "1"}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "183f94cc-f0b6-4283-ae99-dce7b440d974"],
